{
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier",
    "plugin:import/recommended",
    "plugin:import/warnings"
  ],
  // 追記 独自ルールを追加する場合はこれ以降に書く．
  // Prettier を使うので，コメントアウト
  "rules": {
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
    //   // セミコロンない場合、エラー出力
    //   "semi": ["error", "always"]
  }
}
